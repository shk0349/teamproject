<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE tiles-definitions PUBLIC
"-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
"http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
   <definition name="baseLayout" template="/WEB-INF/views/layout.jsp">
      <put-attribute name="title" value=""/>
      <put-attribute name="top" value="/WEB-INF/views/top.jsp"/>
      <put-attribute name="body" value=""/>
      <put-attribute name="footer" value="/WEB-INF/views/footer.jsp"/>
   </definition>
   
   <definition name="main" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/main.jsp"/>
   </definition> 
   <!-- FAQ -->
   <!-- 문의글 작성 -->
   <definition name="faqinput" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/faq/faqinput.jsp"/>
   </definition>
   <!-- 문의글 목록 -->
   <definition name="faqoutput" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/faq/faqoutput.jsp"/>
   </definition>
   <!-- 문의글 상세페이지 -->
   <definition name="faqDtailview" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/faq/faqDtailview.jsp"/>
   </definition>
   <!-- 문의글 상세페이지 수정 -->
   <definition name="faqUpdateview" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/faq/faqUpdateview.jsp"/>
   </definition>
   <!-- 문의글 답변 -->
   <definition name="faqreplyview" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/faq/faqreplyview.jsp"/>
   </definition>
   
   <!-- 공지사항(notice) -->
   <!-- 공지글 작성 -->
   <definition name="notice_input" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/notice/notice_input.jsp"/>
   </definition>
   <!-- 공지글 출력 -->
   <definition name="notice_output" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/notice/notice_output.jsp"/>
   </definition>
   <!-- 공지글 상세 페이지 -->
   <definition name="notice_Detailview" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/notice/notice_Detailview.jsp"/>
   </definition>
   <!-- 공지글 수정 페이지 -->
   <definition name="notice_Updateview" extends="baseLayout">
      <put-attribute name="title" value="메인화면"/>
      <put-attribute name="body" value="/WEB-INF/views/notice/notice_Updateview.jsp"/>
   </definition>
   
   
</tiles-definitions>


















